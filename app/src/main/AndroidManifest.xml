        <activity android:name="pooa20171.iff.br.despesa.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pooa20171.iff.br.despesa.activity.DespesaActivity"
